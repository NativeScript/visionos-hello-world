<Page xmlns="http://schemas.nativescript.org/tns.xsd" navigatingTo="navigatingTo" xmlns:sui="@nativescript/swift-ui">
    <ActionBar title="NativeScript Vision Pro Starter" icon="" />

    <StackLayout class="p-5">
        <Label text="Tap the button" class="text-lg text-center" />
        <Button text="TAP" tap="{{ onTap }}" class="rounded-full  text-white bg-sky-500 my-5 p-2 text-xl" />
        <Label text="{{ message }}" class="text-center" textWrap="true" />
        <sui:SwiftUI swiftId="visionPro" />
    </StackLayout>
</Page>
